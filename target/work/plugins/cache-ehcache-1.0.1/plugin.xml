<plugin name='cache-ehcache' version='1.0.1' grailsVersion='2.0 &gt; *'>
  <author>Burt Beckwith</author>
  <authorEmail>burt@burtbeckwith.com</authorEmail>
  <title>Ehcache Cache Plugin</title>
  <description>An Ehcache-based implementation of the Cache plugin</description>
  <documentation>http://grails-plugins.github.io/grails-cache-ehcache/</documentation>
  <type>CacheEhcacheGrailsPlugin</type>
  <resources />
  <repositories>
    <repository name='grailsCentral' url='http://grails.org/plugins' />
    <repository name='mavenCentral' url='http://repo1.maven.org/maven2/' />
  </repositories>
  <dependencies>
    <test>
      <dependency group='org.codehaus.jsr166-mirror' name='jsr166y' version='1.7.0' />
      <dependency group='org.codehaus.gpars' name='gpars' version='1.0.0' />
    </test>
    <compile>
      <dependency group='net.sf.ehcache' name='ehcache' version='2.8.1' />
    </compile>
  </dependencies>
  <plugins>
    <compile>
      <plugin group='org.grails.plugins' name='cache' version='1.1.1' />
    </compile>
  </plugins>
  <runtimePluginRequirements />
  <behavior />
</plugin>